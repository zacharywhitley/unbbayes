            >1</owl:cardinality>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:about="#isSubsBy"/>
            </owl:onProperty>
          </owl:Restriction>
          <owl:Restriction>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:about="#isSubsBy"/>
            </owl:onProperty>
            <owl:allValuesFrom>
              <owl:Class rdf:about="#MetaEntity"/>
            </owl:allValuesFrom>
          </owl:Restriction>
