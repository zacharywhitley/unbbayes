          </owl:Restriction>
          <owl:Restriction>
            <owl:someValuesFrom>
              <owl:Class rdf:about="#MetaEntity"/>
            </owl:someValuesFrom>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:ID="isSubsBy"/>
            </owl:onProperty>
          </owl:Restriction>
          <owl:Restriction>
            <owl:someValuesFrom rdf:resource="#ArgRelationship"/>
            <owl:onProperty>
              <owl:InverseFunctionalProperty rdf:about="#isArgTermIn"/>
            </owl:onProperty>
          </owl:Restriction>
          <owl:Restriction>
            <owl:onProperty>
