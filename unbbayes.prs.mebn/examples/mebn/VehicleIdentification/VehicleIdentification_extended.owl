      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class>
            <owl:oneOf rdf:parseType="Collection">
              <BooleanRVState rdf:about="#true"/>
              <BooleanRVState rdf:about="#absurd"/>
            </owl:oneOf>
          </owl:Class>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasPossibleValues"/>
        </owl:onProperty>
