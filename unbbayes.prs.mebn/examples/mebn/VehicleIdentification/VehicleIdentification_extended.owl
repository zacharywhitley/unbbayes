      Medium = .05,
      Fast = .85,
      VeryFast = .1
   ] else if any rgn have ( TerrainType = OffRoad ) [
      Stationary = 0,
      Slow = 0,
      Medium = .1,
      Fast = .85,
      VeryFast = .05
   ] else [
      Stationary = 0,
      Slow = 0,
      Medium = .15,
      Fast = .8,
      VeryFast = .05
   ] 
] else if any obj have ( Speed = VeryFast ) [
   if any rgn have ( TerrainType = Road ) [
      Stationary = 0,
      Slow = 0,
      Medium = 0,
      Fast = .05,
      VeryFast = .95
   ] else if any rgn have ( TerrainType = OffRoad ) [
      Stationary = 0,
      Slow = 0,
      Medium = 0,
      Fast = .15,
      VeryFast = .85
   ] else [
      Stationary = 0,
      Slow = 0,
      Medium = 0,
      Fast = .1,
      VeryFast = .9
   ] 
] else [
   if any rgn have ( TerrainType = Road ) [
      Stationary = 0,
      Slow = 0,
      Medium = .2,
      Fast = .8,
      VeryFast = 0
   ] else if any rgn have ( TerrainType = OffRoad ) [
      Stationary = 0,
      Slow = .8,
      Medium = .2,
      Fast = 0,
      VeryFast = 0
   ] else [
      Stationary = 0,
      Slow = .2,
      Medium = .8,
      Fast = 0,
      VeryFast = 0
   ] 
] 


