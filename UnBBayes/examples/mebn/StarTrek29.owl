        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#hasParentName"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="isConditionantOf"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="ProbAssign"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
