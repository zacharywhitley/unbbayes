            >1</owl:maxCardinality>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:about="#isArgumentOf"/>
            </owl:onProperty>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:about="#Input">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
