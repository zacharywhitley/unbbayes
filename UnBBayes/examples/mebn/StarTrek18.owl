      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="isConditionantOf"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="ProbAssign"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
