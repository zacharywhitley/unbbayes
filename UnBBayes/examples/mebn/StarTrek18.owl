        <owl:someValuesFrom rdf:resource="#CondRelationship"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#Entity"/>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#hasStateName"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:ID="hasStateProb"/>
