<?xml version="1.0" encoding="UTF-8"?><project>
  <modelVersion>4.0.0</modelVersion>
  <groupId>br.unb.cic</groupId>
  <artifactId>unbbayesbayesontology</artifactId>
  <name>UnBBayesBayesOntology</name>
  <version>3.1-SNAPSHOT</version>
  <url>http://unbbayes.sourceforge.net</url>
  <inceptionYear>2001</inceptionYear>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>1.5</source>
          <target>1.5</target>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-site-plugin</artifactId>
        <configuration>
          <locales>en</locales>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <dependencies>
    <dependency>
      <groupId>javax.help</groupId>
      <artifactId>javahelp</artifactId>
      <version>2.0.02</version>
    </dependency>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
    </dependency>
    <dependency>
      <groupId>ptolomy.plot</groupId>
      <artifactId>plot</artifactId>
      <version>1.0</version>
    </dependency>
    <dependency>
      <groupId>xalan</groupId>
      <artifactId>xalan</artifactId>
      <version>2.7.0</version>
    </dependency>
    <dependency>
      <groupId>xml-apis</groupId>
      <artifactId>xml-apis</artifactId>
      <version>1.0.2</version>
    </dependency>
    <dependency>
      <groupId>edu.stanford.smi</groupId>
      <artifactId>protege</artifactId>
      <version>1.0</version>
    </dependency>
  </dependencies>
  <reporting>
    <plugins>
      <plugin>
        <artifactId>maven-surefire-report-plugin</artifactId>
      </plugin>
    </plugins>
  </reporting>
  <distributionManagement>
    <site>
      <id>website</id>
      <url>ftp://sourceforge.net/www/u/un/unbbayes</url>
    </site>
  </distributionManagement>
</project>