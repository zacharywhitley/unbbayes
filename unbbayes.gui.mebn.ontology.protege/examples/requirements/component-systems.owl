    <!-- http://www.owl-ontologies.com/requirements/component-systems.owl#AtomicComponent -->

    <owl:Class rdf:about="http://www.owl-ontologies.com/requirements/component-systems.owl#AtomicComponent">
        <rdfs:label rdf:datatype="&xsd;string">AtomicComponent</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.owl-ontologies.com/requirements/component-systems.owl#Component"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.owl-ontologies.com/requirements/component-systems.owl#hasPart"/>
                <owl:allValuesFrom rdf:resource="http://www.owl-ontologies.com/requirements/component-systems.owl#Connector"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string">An atomic component may not contain other components</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.owl-ontologies.com/requirements/component-systems.owl#AtomicPart -->

    <owl:Class rdf:about="http://www.owl-ontologies.com/requirements/component-systems.owl#AtomicPart">
