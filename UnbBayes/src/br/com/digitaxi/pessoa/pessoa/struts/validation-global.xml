    <field property="cpf"
        depends="required" >
          <arg0 resource="false" key="CPF"/>         	
     </field>
    <field property="usuario"
        depends="required" >
          <arg0 resource="false" key="Usuário"/>         	
     </field>

</form>
</formset>

</form-validation>