<?xml version="1.0" encoding="ISO-8859-1" ?>

<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">

<struts-config>

<form-beans>

<form-bean 
    name="pessoaForm" 
    type="org.apache.struts.action.DynaActionForm">

	<form-property name="idPessoa" type="java.lang.String" />
	<form-property name="usuario" type="java.lang.String" />
	<form-property name="nome" type="java.lang.String" />
	<form-property name="cpf" type="java.lang.String" />
	<form-property name="telefoneRes" type="java.lang.String" />
	<form-property name="celular" type="java.lang.String" />
	<form-property name="telefoneCom" type="java.lang.String" />
	<form-property name="email" type="java.lang.String" />
	<form-property name="descricao" type="java.lang.String" />

	<form-property name="nomePesquisa" type="java.lang.String" />

</form-bean>

</form-beans>

</struts-config>
    