	<build>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.5</source>
					<target>1.5</target>
				</configuration>
			</plugin>
			<plugin>
				<artifactId>maven-assembly-plugin</artifactId>
				<configuration>
					<descriptors>
						<descriptor>dist.xml</descriptor>
					</descriptors>
				</configuration>
			</plugin>
			<plugin>
				<artifactId>maven-site-plugin</artifactId>
				<configuration>
					<locales>en</locales>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.ws.jaxme</groupId>
				<artifactId>maven-jaxme-plugin</artifactId>
				<executions>
					<execution>
						<goals>
							<goal>jaxme</goal>
						</goals>
					</execution>
				</executions>
				<configuration>
					<schemas>
						<value>src/main/xsd/XMLBIF_0_7.xsd
						</value>
					</schemas>
					<packageName>unbbayes.io.xmlbif.version7.xmlclasses
					</packageName>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<dependencies>
		<!--
			This is a plugin project for UnBBayes. Since it uses classes from the
			core, UnBBayes is indeed a dependency
		-->
		<dependency>
			<groupId>br.unb.cic</groupId>
			<artifactId>unbbayes</artifactId>
			<version>4.0.0</version>
		</dependency>

		<!--
			This plugin has a dependency to, prognos.unbbayesplugin.cps plugin
		-->
		<dependency>
			<groupId>edu.gmu.seor</groupId>
			<artifactId>prognos.unbbayesplugin.cps
			</artifactId>
			<version>1.0.0</version>
		</dependency>

		<!-- JAXME/JAXB -->
		<!--
			JaxMe 2 is an open source implementation of JAXB, the specification
			for Java/XML binding. http://ws.apache.org/jaxme/
		-->
		<dependency>
			<groupId>jaxme</groupId>
			<artifactId>jaxme2</artifactId>
			<version>0.5.1</version>
		</dependency>
		<dependency>
			<groupId>jaxme</groupId>
			<artifactId>jaxme2-rt</artifactId>
			<version>0.5.1</version>
		</dependency>
		<dependency>
			<groupId>jaxme</groupId>
			<artifactId>jaxmeapi</artifactId>
			<version>0.5.1</version>
		</dependency>
		<dependency>
			<groupId>jaxme</groupId>
			<artifactId>jaxmejs</artifactId>
			<version>0.5.1</version>
		</dependency>
		<dependency>
			<groupId>jaxme</groupId>
			<artifactId>jaxmexs</artifactId>
			<version>0.5.1</version>
		</dependency>
		<!-- JAXME/JAXB -->

	</dependencies>
